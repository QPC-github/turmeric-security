<schema targetNamespace="http://www.ebayopensource.org/turmeric/services/groupmembership/config"
	elementFormDefault="qualified"
	xmlns="http://www.w3.org/2001/XMLSchema"
	xmlns:tns="http://www.ebayopensource.org/turmeric/services/groupmembership/config">
    <element name="group-membership-service-provider-config" type="tns:GroupMembershipServiceProviderConfig"/>
    	
    <complexType name="GroupMembershipServiceProviderConfig">
    	<sequence>
    		<element name="default" type="string" ></element>    	
     		<element name="provider-config-list" type="tns:GroupMembershipServiceProviderList"/>
    	</sequence>
    </complexType>
    <complexType name="GroupMembershipServiceProviderList">
    	<sequence>  	
     		<element name="provider-config" type="tns:GroupMembershipServiceProvider" 
     		minOccurs="1" maxOccurs="unbounded"/>
    	</sequence>
    </complexType>
    <complexType name="GroupMembershipServiceProvider">
    	<sequence>
    		<element name="provider-name" type="string"></element>    	
     		<element name="provider-impl-classname" type="token"/>
    	</sequence>
    </complexType>
</schema>
